    <!-- http://www.semanticweb.org/antca/ontologies/2023/4/untitled-ontology-4#istanza_mal_di_testa -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/antca/ontologies/2023/4/untitled-ontology-4#istanza_mal_di_testa">
